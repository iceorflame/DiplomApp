<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-type" content="text/html; charset=utf-8">
    <meta name="robots" content="noindex,follow">
    <title>@ViewBag.Title</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/default.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Contentlogin.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/dropzone-4.3.0.min.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/bootstrap-3.3.6.min.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/font-awesome-4.6.3.min.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/fonts.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/bootstrap-datetimepicker-4.17.47.min.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/ace.min.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/ace-mantis.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/ace-skins.min.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/bootstrap-datetimepicker.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/bootstrap-datetimepicker.min.css")">
    @RenderSection("Styles", false)
    <link rel="shortcut icon" href="~/favicon.ico" type="image/x-icon">
</head>
@RenderBody()
<script type="text/javascript" src="@Url.Content("~/Scripts/jquery-2.2.4.min.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/dropzone-4.3.0.min.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/common.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/bootstrap-3.3.6.min.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/moment-with-locales-2.15.2.min.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/bootstrap-datetimepicker-4.17.47.min.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/typeahead.jquery-1.1.1.min.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/list-1.4.1.min.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/ace.min.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/bootstrap-datetimepicker.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/bootstrap-datetimepicker.min.js")"></script>
@RenderSection("Scripts", required: false)
</html>
